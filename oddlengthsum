class Solution {
    public int sumOddLengthSubarrays(int[] arr) {
       int total = 0;
        int len = arr.length;
        for(int i=0; i<len; i++) {
            int sum = 0;
            for(int j=i; j<len; j++) {
                sum += arr[j];
                if(((j-i)+1) % 2 == 1)
                    total += sum;
            }
        }
        return total;
    }
}